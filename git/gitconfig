[alias]
	br = branch
	ci = commit
	cm = commit -a -m
	co = checkout
	com = checkout master
	cod = checkout develop
	cof = checkout feature
	cob = checkout bugfix
	cor = checkout release
	df = diff
	di = diff
	did = diff develop upstream/develop
	dt = difftool
    fi = flow init
    fli = flow init
    flfs = flow feature start
    flff = flow feature finish
    flrs = flow release start
    flrf = flow release finish
    fs = flow feature start
    frs = flow release start
    frf = flow release finish
    ff = flow feature finish
	ft = fetch
	fto = fetch origin
	ftu = fetch upstream
	pl = pull
	ps = push
	pu = push
	md = merge --no-ff dev
	mnf = merge --no-ff
	st = status
	unstage = reset HEAD --
	us = reset HEAD --
	last = log -1 HEAD
	info = remote -v
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
