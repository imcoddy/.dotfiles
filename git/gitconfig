[core]
	whitespace = trailing-space,space-before-tab,indent-with-non-tab
	editor = vim
	excludesfile = ~/.gitignore
[color]
	ui = true
	diff = auto
	status = auto
[alias]
	br = branch

	ci = commit
	cm = commit -a -m

	co = checkout
	com = checkout master
	cod = checkout develop
	cof = checkout feature
	cob = checkout bugfix
	cor = checkout release

	df = diff
	dft = difftool
	di = diff
	did = diff develop upstream/develop
	dt = difftool

    fl = flow
    fi = flow init
    fli = flow init
    flfs = flow feature start
    flff = flow feature finish
    flrs = flow release start
    flrf = flow release finish
    fs = flow feature start
    frs = flow release start
    frf = flow release finish
    ff = flow feature finish

	ft = fetch
	fto = fetch origin
	ftu = fetch upstream

	info = remote -v

	last = log -1 HEAD

	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --

	md = merge --no-ff dev
	mnf = merge --no-ff

	pl = pull
	ps = push
	pu = push

	reset = reset HEAD --
	st = status
	unstage = reset HEAD --
	us = reset HEAD --
